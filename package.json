{
  "name": "test-bybit-bot",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:k-fujino-kohei/test-bybit-bot.git",
  "author": "Kohei Fujino <appleblueberrycherry@gmail.co.jp>",
  "license": "MIT",
  "scripts": {
    "lint:ts": "tsc --noEmit",
    "lint:es": "eslint --ext \".ts\" --ignore-path .gitignore .",
    "fix": "yarn lint:es --fix",
    "bundle": "aspida && esbuild src/index.ts --bundle --outfile=index.js --minify --platform=node --external:mock-aws-s3 --external:nock",
    "local": "yarn bundle && node index.js",
    "test:bundle": "aspida && esbuild src/backtest.ts --bundle --outfile=backtest.js --platform=node --external:mock-aws-s3 --external:nock",
    "backtest": "yarn test:bundle && node backtest.js",
    "crawl:bundle": "aspida && esbuild src/bin/crawl.ts --bundle --outfile=crawl.js --platform=node --external:mock-aws-s3 --external:nock",
    "crawl": "yarn crawl:bundle && node crawl.js"
  },
  "dependencies": {
    "@aspida/axios": "^1.8.0",
    "@supabase/supabase-js": "^1.33.3",
    "@types/node-cron": "^3.0.1",
    "axios": "^0.26.0",
    "crypto-js": "^4.1.1",
    "csv": "^6.0.5",
    "date-fns": "^2.28.0",
    "dotenv": "^11.0.0",
    "ringbufferjs": "^2.0.0",
    "snake-camel": "^1.0.8"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^17.0.8",
    "@types/ringbufferjs": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "bybit-api": "^2.1.8",
    "esbuild": "^0.14.11",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "node-cron": "^3.0.0",
    "typescript": "^4.5.4"
  }
}
